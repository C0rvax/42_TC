{
  "dashboard": {
    "id": null,
    "uid": "cadvisor-dashboard",
    "title": "cAdvisor Metrics",
    "tags": ["cadvisor", "docker"],
    "timezone": "browser",
    "panels": [
      {
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{job=\"cadvisor\"}[1m])",
            "legendFormat": "{{container}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxis": {
          "format": "percent"
        }
      },
      {
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{job=\"cadvisor\"}",
            "legendFormat": "{{container}}",
            "refId": "B"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxis": {
          "format": "bytes"
        }
      }
    ]
  },
  "overwrite": false
}
